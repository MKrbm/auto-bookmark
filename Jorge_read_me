 # いったんpythonを使用する
 pwd
/home/jorge/project/autobook/auto-bookmark/python
pyenv install 3.10.0
pyenv virtualenv 3.10.0 jorge_env
pyenv activate jorge_env

# 必要なmoduleのinstall
echo "beautifulsoup4" >> requirements.txt
echo "langchain_community" >> requirements.txt
echo "bs4" >> requirements.txt
pip install -r requirements.txt


 環境変数設定
 export PATH="$HOME/.pyenv/bin:$PATH"
 eval "$(pyenv init --path)"
 eval "$(pyenv init -)"
 eval "$(pyenv virtualenv-init -)"
 source ~/.bashrc



 # RAGをつくるよ
npm install dotenv openai
npm install --save-dev typescript
npm install dotenv




# ファイルを実行
typescript/配下に移動
npm install @langchain/langgraph @langchain/community @langchain/anthropic @langchain/core
npm install langchain chroma-js

npm install
npm run build


